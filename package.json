{
  "name": "waifu-bot",
  "version": "3.2.403",
  "description": "Waifu Bot (Currently Shikimori) is multipurpose Discord Bot with Music that serves our server",
  "main": "index.js",
  "author": "Leomotors",
  "license": "MIT",
  "type": "module",
  "repository": "https://github.com/Leomotors/waifu-bot",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist/*",
    "cb": "yarn clean && yarn build",
    "start": "node --es-module-specifier-resolution=node dist/bot.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src && prettier --check \"src/**/*.ts\""
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/node": "^18.6.4",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@discordjs/opus": "^0.8.0",
    "@leomotors/music-bot": "^0.15.1",
    "@yimura/scraper": "^1.2.3",
    "chalk": "^5.0.1",
    "cocoa-discord-utils": "^2.2.0",
    "discord.js": "^14.1.2",
    "dotenv": "^16.0.1",
    "google-tts-api": "^2.0.2",
    "libsodium-wrappers": "^0.7.10",
    "node-fetch": "^3.2.10",
    "uuid": "^8.3.2"
  },
  "packageManager": "yarn@3.2.2"
}
