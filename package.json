{
  "name": "waifu-bot",
  "description": "Waifu Bot is multipurpose Discord Bot with Music that serves our server",
  "repository": "https://github.com/Leomotors/waifu-bot",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "postinstall": "prisma generate",
    "clean": "rm -rf node_modules {apps,packages}/**/*/{.turbo,build,dist,node_modules,.next}",
    "build": "FORCE_COLOR=2 turbo run build",
    "dev": "FORCE_COLOR=2 turbo run dev",
    "lint": "FORCE_COLOR=2 turbo run lint",
    "format": "FORCE_COLOR=2 turbo run format",
    "start": "FORCE_COLOR=2 turbo run start",
    "start:bot": "FORCE_COLOR=2 turbo run --filter=\"@waifu-bot/bot\" start",
    "test": "FORCE_COLOR=2 turbo run test"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^3.7.1",
    "@leomotors/config": "^0.7.0",
    "@nestjs/cli": "^9.1.5",
    "@prisma/client": "^4.7.1",
    "@types/eslint": "^8.4.10",
    "@types/node": "^18.11.15",
    "@types/prettier": "^2.7.1",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.29.0",
    "eslint-config-next": "^13.0.7",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "prisma": "^4.7.1",
    "prisma-nestjs-graphql": "^17.0.3",
    "turbo": "^1.6.3",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": "^18.12.0",
    "pnpm": "^7"
  }
}