{
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**",
        ".svelte-kit/**",
        "build/**"
      ]
    },
    "lint": {},
    "format": {},
    "start": {
      "dependsOn": [
        "build",
        "lint"
      ],
      "cache": false
    },
    "test": {
      "dependsOn": [
        "build"
      ]
    },
    "dev": {
      "cache": false
    }
  }
}