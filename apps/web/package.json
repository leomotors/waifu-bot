{
  "name": "@waifu-bot/web",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 7100",
    "build": "vite build",
    "preview": "vite preview",
    "start": "PORT=7100 HOST=0.0.0.0 ORIGIN=$PUBLIC_WEB_URL node build/index.js",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest --run",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^5.2.12",
    "@sveltejs/kit": "^2.17.2",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "@urami/core": "^1.3.0",
    "@urami/svelte": "^1.2.0",
    "@urami/types": "^1.1.0",
    "@waifu-bot/constants": "workspace:*",
    "autoprefixer": "^10.4.20",
    "discord-api-types": "^0.37.114",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.1",
    "jsonwebtoken": "^9.0.2",
    "prettier": "^3.5.2",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "sass": "^1.85.0",
    "svelte": "^5.20.2",
    "svelte-bootstrap-icons": "^3.1.2",
    "svelte-check": "^4.1.4",
    "tailwind-merge": "^2.5.5",
    "tailwindcss": "^3.4.17",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "vite": "^6.1.1",
    "vitest": "^3.0.6",
    "zod": "^3.24.2"
  },
  "dependencies": {
    "@waifu-bot/auth": "workspace:*",
    "@waifu-bot/database": "workspace:*",
    "sharp": "^0.33.5"
  }
}
