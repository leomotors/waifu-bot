{
  "name": "@waifu-bot/web",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 7100",
    "build": "vite build",
    "preview": "vite preview",
    "start": "PORT=7100 HOST=0.0.0.0 ORIGIN=$PUBLIC_WEB_URL node build/index.js",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest --run",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "4.0.1",
    "@sveltejs/kit": "2.5.0",
    "@sveltejs/vite-plugin-svelte": "3.0.2",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/jsonwebtoken": "9.0.5",
    "@typescript-eslint/eslint-plugin": "6.20.0",
    "@typescript-eslint/parser": "6.20.0",
    "@urami/core": "1.2.3",
    "@urami/svelte": "1.1.1",
    "@urami/types": "1.0.1",
    "@waifu-bot/constants": "workspace:*",
    "autoprefixer": "10.4.17",
    "discord-api-types": "0.37.67",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-svelte": "2.35.1",
    "jsonwebtoken": "9.0.2",
    "prettier": "3.2.5",
    "prettier-plugin-svelte": "3.1.2",
    "prettier-plugin-tailwindcss": "0.5.11",
    "sass": "1.70.0",
    "svelte": "4.2.9",
    "svelte-bootstrap-icons": "3.1.1",
    "svelte-check": "3.6.3",
    "tailwind-merge": "2.2.1",
    "tailwindcss": "3.4.1",
    "tslib": "2.6.2",
    "typescript": "5.3.3",
    "vite": "5.0.12",
    "vitest": "1.2.2",
    "zod": "3.22.4"
  },
  "dependencies": {
    "@waifu-bot/auth": "workspace:*",
    "@waifu-bot/database": "workspace:*",
    "sharp": "0.33.2"
  }
}
