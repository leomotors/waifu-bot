{
  "name": "@waifu-bot/server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "dev": "nest start --watch",
    "dev:debug": "nest start --debug --watch",
    "start": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --max-warnings=0",
    "format": "prettier --write \"src/**/*.ts\"",
    "deps": "ncu -u"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.0.17",
    "@nestjs/common": "^9.0.3",
    "@nestjs/core": "^9.0.3",
    "@nestjs/graphql": "^10.0.18",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^9.0.3",
    "@prisma/client": "^4.0.0",
    "@waifu-bot/constants": "*",
    "apollo-server-core": "^3.10.0",
    "apollo-server-express": "^3.10.0",
    "class-transformer": "^0.5.1",
    "express": "^4.18.1",
    "graphql": "^16.5.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "@waifu-bot/config": "*",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "prisma": "^4.0.0",
    "prisma-dbml-generator": "^0.9.1",
    "prisma-nestjs-graphql": "^16.0.1",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
