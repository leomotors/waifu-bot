{
  "name": "@waifu-bot/bot",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist/*",
    "cb": "yarn clean && yarn build",
    "start": "node --es-module-specifier-resolution=node dist/apps/bot/src/bot.js",
