    "@waifu-bot/config": "*",
    "eslint": "^8.21.0",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@discordjs/opus": "^0.8.0",
    "@leomotors/music-bot": "^0.15.1",
    "@waifu-bot/constants": "*",
    "@waifu-bot/graphql": "*",
    "@yimura/scraper": "^1.2.3",
    "chalk": "^5.0.1",
    "cocoa-discord-utils": "^2.2.0",
    "discord.js": "^14.1.2",
    "dotenv": "^16.0.1",
    "google-tts-api": "^2.0.2",
    "graphql": "^16.5.0",
    "graphql-request": "^4.3.0",
    "libsodium-wrappers": "^0.7.10",
