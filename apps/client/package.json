{
  "name": "@waifu-bot/client",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "prepare": "svelte-kit sync",
    "start": "PORT=5374 node build",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check \"*.config.{cjs,js,ts}\" \"{src,tests}/**/*.{ts,svelte,html,scss}\" && eslint src *.{cjs,js}",
    "format": "prettier --write \"*.config.{cjs,js,ts}\" \"{src,tests}/**/*.{ts,svelte,html,scss}\"",
    "generate": "graphql-codegen",
    "deps": "ncu -u"
  },
  "devDependencies": {
    "@apollo/client": "^3.6.9",
    "@graphql-codegen/cli": "^2.11.3",
    "@graphql-codegen/typescript": "^2.7.2",
    "@graphql-codegen/typescript-operations": "^2.5.2",
    "@sveltejs/adapter-node": "^1.0.0-next.85",
    "@sveltejs/kit": "1.0.0-next.398",
    "@types/eslint": "^8.4.5",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "autoprefixer": "^10.4.8",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "graphql": "^16.5.0",
    "graphql-codegen-svelte-apollo": "^1.1.0",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "sass": "^1.54.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.7",
    "typescript": "^4.7.4",
    "vite": "^3.0.4"
  }
}